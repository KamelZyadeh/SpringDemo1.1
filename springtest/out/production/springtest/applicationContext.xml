<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="student-bean" class="com.springDemo.Student">
        <property name="name" value="Ziad"/>
        <!--<constructor-arg type="java.lang.String" value="Ziad"/>-->
    </bean>

    <bean id="teacher-bean" class="com.springDemo.Teacher">
        <property name="name" value="kamel"/>
    </bean>

    <bean id="substitute-bean" class="com.springDemo.Substitute">
        <property name="name" value="Ali"/>
    </bean>

    <bean id="Janitor-bean" class="com.springDemo.Janitor">
        <property name="name" value="Magdy"/>
    </bean>

    <bean id="principal-bean" class="com.springDemo.Principal">
        <property name="name" value="KAMEL"/>
    </bean>

</beans>